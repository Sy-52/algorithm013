public static void main (String[] args) {
class Solution {
    public void rotate(int[] nums, int k) {
        for (int i = 0; i < k; i++) {
            int record = nums[nums.length - 1];
            for (int j = 0; j < nums.length - 1; j++) {
                nums[j + 1] = nums[j];
            }
            nums[0] = record;
        }
    }
}
    Solution.rotate([1,2,3,4,5,6,7],3);
}

